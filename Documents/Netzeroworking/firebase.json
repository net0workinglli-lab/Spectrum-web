{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/u/**",
        "function": "redirectCard"
      },
      {
        "source": "/vcard.html",
        "destination": "/vcard.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],"headers": [
    {
      "source": "/vcard.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    }
  ]
  },
  "functions": {
    "source": "functions"
  }
}
